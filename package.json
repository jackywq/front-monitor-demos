{
  "name": "frontend-monitor-sdk",
  "version": "1.0.0",
  "description": "轻量级前端监控 SDK，支持错误监控、性能监控、用户行为监控",
  "main": "monitor/monitor-sdk.js",
  "scripts": {
    "build:monitor": "cd monitor-core && npm run build",
    "monitor:install": "cd monitor-core && npm install",
    "monitor:dev": "cd monitor-core && npm run dev",
    "dashboard:install": "cd monitor-dashboard && npm install",
    "dashboard:start": "cd monitor-dashboard && npm start",
    "dashboard:dev": "cd monitor-dashboard && npm run dev",
    "dashboard-react:install": "cd monitor-dashboard && npm install",
    "dashboard-react:dev": "cd monitor-dashboard && npm run dev",
    "dashboard-react:build": "cd monitor-dashboard && npm run build",
    "dashboard-react:serve": "cd monitor-dashboard && npm run serve",
    "playground:install": "cd monitor-playground && npm install",
    "playground:dev": "cd monitor-playground && npm run dev",
    "playground:build": "cd monitor-playground && npm run build",
    "playground:preview": "cd monitor-playground && npm run preview",
    "start": "concurrently \"npm run dashboard:start\" \"npm run playground:dev\" \"npm run dashboard-react:dev\"",
    "start:react": "concurrently \"npm run dashboard:start\" \"npm run playground:dev\" \"npm run dashboard-react:dev\"",
    "start:legacy": "concurrently \"npm run dashboard:start\" \"npm run playground:dev\"",
    "install:all": "npm run monitor:install && npm run dashboard:install && npm run playground:install && npm run dashboard-react:install",
    "build:all": "npm run build:monitor && npm run playground:build && npm run dashboard-react:build"
  },
  "keywords": [
    "monitor",
    "frontend",
    "error",
    "performance",
    "analytics",
    "sdk"
  ],
  "author": "Frontend Monitor Team",
  "license": "MIT",
  "devDependencies": {
    "uglify-js": "^3.17.4",
    "concurrently": "^7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-repo/frontend-monitor-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/your-repo/frontend-monitor-sdk/issues"
  },
  "homepage": "https://github.com/your-repo/frontend-monitor-sdk#readme"
}
